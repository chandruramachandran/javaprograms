import java.util.*;
public class Main {
     public static void main(String args[]){
         Scanner e = new Scanner(System.in);
         int a = e.nextInt();
         int b = e.nextInt();
         int count = 10;
         System.out.println(a+" "+b);
         for(int i=2; i<=count; i++){
             int c = a + b;
             System.out.println(c);
              a = b;
              b = c;
         }
     }    
}
