public class Main{
    public static void main(String[] args){
        int n= 5;
        for(int i=0; i<=5; i++){
            for(int j=0; j<=5; j++){
                if(i==0 || i==n || j==0 || j==n)
                {
                    System.out.print("* ");
                }
                else
                {
                    System.out.print("  ");
                }
            }
             System.out.println();
        }
    }
}
