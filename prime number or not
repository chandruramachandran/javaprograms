import java.util.*;
public class Main{
    public static void main(String[] args)
    {
        Scanner a = new Scanner(System.in);
        int b = a.nextInt();
        if(b%1==0 && b%b==0)
        {
            System.out.println("Prime Number");
        }
        else
        {
            System.out.println("Not Prime Number");
        }
    }
}
